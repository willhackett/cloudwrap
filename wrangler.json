{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cloudwrap",
	"main": "src/index.ts",
	"compatibility_date": "2025-02-04",
	"observability": {
		"enabled": true
	},
	"r2_buckets": [
		{
			"binding": "BUCKET",
			"bucket_name": "cloudwrap"
		}
	],
	"assets": {
		"directory": "./public/",
		"binding": "ASSETS"
	}
}
